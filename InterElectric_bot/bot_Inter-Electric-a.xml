<context>  
<input pattern="* (расскажи*|*анекдот*|расскажи анекдот*) *">
            
            <get url="http://rzhunemogu.ru/Rand.aspx" var="Anekdot">                
                <param name="CType" value="1"/>                
            </get>
            
            <var name="Result" value="javascript: $Anekdot.text" />                    
           
            <output value="$Result" if="full($Result)" />          
     
    </input>
   </context>

